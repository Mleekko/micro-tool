{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es6"
    ],
    "module": "Node16",
    "rootDir": "src",
    "resolveJsonModule": true,
    "allowJs": false,
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
